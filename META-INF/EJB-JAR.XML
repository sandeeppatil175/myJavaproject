<?xml version="1.0"?>
<!DOCTYPE ejb-jar PUBLIC '-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 1.1//EN' 'http://java.sun.com/j2ee/dtds/ejb-jar_1_1.dtd'>
<ejb-jar>
	<enterprise-beans>
		<entity>
			<ejb-name>Account</ejb-name>
			<home>project.server.IAccountHome</home>
			<remote>project.server.IAccount</remote>
			<ejb-class>project.server.IAccountBean</ejb-class>
			<persistence-type>Container</persistence-type>
			<prim-key-class>java.lang.String</prim-key-class>
			<reentrant>false</reentrant>
			<cmp-field>
				<field-name>logid</field-name>
			</cmp-field>
			<cmp-field>
				<field-name>login</field-name>
			</cmp-field>
			<cmp-field>
				<field-name>passwd</field-name>
			</cmp-field>
			<cmp-field>
				<field-name>crbal</field-name>
			</cmp-field>
			<cmp-field>
				<field-name>dbbal</field-name>
			</cmp-field>
			<cmp-field>
				<field-name>doj</field-name>
			</cmp-field>
			<primkey-field>logid</primkey-field>
		</entity>
		<entity>
			<ejb-name>Product</ejb-name>
			<home>project.server.IProductHome</home>
			<remote>project.server.IProduct</remote>
			<ejb-class>project.server.IProductBean</ejb-class>
			<persistence-type>Container</persistence-type>
			<prim-key-class>java.lang.String</prim-key-class>
			<reentrant>false</reentrant>
			<cmp-field>
				<field-name>pid</field-name>
			</cmp-field>
			<cmp-field>
				<field-name>pname</field-name>
			</cmp-field>
			<cmp-field>
				<field-name>pprice</field-name>
			</cmp-field>
			<cmp-field>
				<field-name>capacity</field-name>
			</cmp-field>
			<cmp-field>
				<field-name>um</field-name>
			</cmp-field>
			<cmp-field>
				<field-name>producer</field-name>
			</cmp-field>
			<primkey-field>pid</primkey-field>
		</entity>
		<session>
			<ejb-name>ConfirmSession</ejb-name>
			<home>project.client.ConfirmHome</home>
			<remote>project.client.Confirm</remote>
			<ejb-class>project.client.ConfirmBean</ejb-class>
			<session-type>Stateful</session-type>
			<transaction-type>Container</transaction-type>
		</session>
		<session>
			<ejb-name>QuotationSession</ejb-name>
			<home>project.server.QuotationHome</home>
			<remote>project.server.Quotation</remote>
			<ejb-class>project.server.QuotationBean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
		</session>
		<session>
			<ejb-name>CatalogSession</ejb-name>
			<home>project.server.CatalogHome</home>
			<remote>project.server.Catalog</remote>
			<ejb-class>project.server.CatalogBean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
		</session>

	</enterprise-beans>
	
</ejb-jar>
