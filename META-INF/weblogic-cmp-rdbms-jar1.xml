<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE weblogic-rdbms-bean PUBLIC '-//BEA Systems, Inc.//DTD WebLogic 5.1.0 EJB RDBMS Persistence//EN'
 'http://www.bea.com/servers/wls510/dtd/weblogic-rdbms-persistence.dtd'>


<weblogic-rdbms-bean>
	<pool-name>oraclePool</pool-name>
	<table-name>product</table-name>
	<attribute-map>
		<object-link>
			<bean-field>pid</bean-field>
			<dbms-column>pid</dbms-column>
		</object-link>
		<object-link>
			<bean-field>pname</bean-field>
			<dbms-column>pname</dbms-column>
		</object-link>
		<object-link>
			<bean-field>pprice</bean-field>
			<dbms-column>pprice</dbms-column>
		</object-link>
		<object-link>
			<bean-field>capacity</bean-field>
			<dbms-column>capacity</dbms-column>
		</object-link>
		<object-link>
			<bean-field>um</bean-field>
			<dbms-column>unitsofemeasure</dbms-column>
		</object-link>
		<object-link>
			<bean-field>producer</bean-field>
			<dbms-column>producer</dbms-column>
		</object-link>
	</attribute-map>
	<options>
		<use-quoted-names>false</use-quoted-names>
	</options>
</weblogic-rdbms-bean>

