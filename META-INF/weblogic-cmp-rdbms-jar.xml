<!DOCTYPE weblogic-rdbms-bean PUBLIC '-//BEA Systems, Inc.//DTD WebLogic 5.1.0 EJB RDBMS Persistence//EN'
 'http://www.bea.com/servers/wls510/dtd/weblogic-rdbms-persistence.dtd'>
<weblogic-rdbms-bean>
	<pool-name>oraclePool</pool-name>
	<table-name>account1</table-name>
	<attribute-map>
		<object-link>
			<bean-field>logid</bean-field>
			<dbms-column>logid</dbms-column>
		</object-link>
		<object-link>
			<bean-field>login</bean-field>
			<dbms-column>login</dbms-column>
		</object-link>
		<object-link>
			<bean-field>passwd</bean-field>
			<dbms-column>passwd</dbms-column>
		</object-link>
		<object-link>
			<bean-field>crbal</bean-field>
			<dbms-column>crbal</dbms-column>
		</object-link>
		<object-link>
			<bean-field>dbbal</bean-field>
			<dbms-column>dbbal</dbms-column>
		</object-link>
		<object-link>
			<bean-field>doj</bean-field>
			<dbms-column>doj</dbms-column>
		</object-link>
	</attribute-map>
	<options>
		<use-quoted-names>false</use-quoted-names>
	</options>
	</weblogic-rdbms-bean>

